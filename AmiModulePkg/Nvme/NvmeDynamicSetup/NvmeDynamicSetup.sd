        goto
            formsetguid = NVME_FORM_SET_GUID,
            formid = NVME_MAIN_FORM_ID,
            question = 0,
            prompt = STRING_TOKEN(STR_NVME_CONFIG_FORM),
            help = STRING_TOKEN(STR_NVME_CONFIG_FORM_HELP);
    #endif
#endif

