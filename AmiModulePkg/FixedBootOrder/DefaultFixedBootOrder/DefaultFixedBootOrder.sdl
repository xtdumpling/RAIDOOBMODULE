TOKEN
    Name  = "DEFAULT_FIXED_BOOT_ORDER_SUPPORT"
    Value  = "1"
    Help  = "Main switch to enable module part support in Project"
    TokenType = Boolean
    TargetEQU = Yes
    TargetMAK = Yes
    TargetH = Yes
    Master = Yes
End

