End

MODULE
    Help  = "Includes DefaultFixedBootOrder.mak to Project"
    File  = "DefaultFixedBootOrder.mak"
End

TOKEN
    Name  = "D_OR_SYMBOL"
    Value  = "||"
    Help  = "The Gawk.exe can't parsing |(or) symbol, so we need the macro to replace it in sd file."
    TokenType = Expression
    TargetH = Yes
End

TOKEN
    Name  = "OR_SYMBOL"
    Value  = "|"
    Help  = "The Gawk.exe can't parsing |(or) symbol, so we need the macro to replace it in sd file."
    TokenType = Expression
    TargetH = Yes
End

