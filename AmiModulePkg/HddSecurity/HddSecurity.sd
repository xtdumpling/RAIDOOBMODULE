                prompt      = STRING_TOKEN(STR_IDE_MASTER_PASSWORD),\
                help        = STRING_TOKEN(STR_IDE_MASTER_PASSWORD_HELP),\
                flags       = INTERACTIVE | RESET_REQUIRED,\
                key         = KEY_VALUE,\
                minsize     = 0,\
