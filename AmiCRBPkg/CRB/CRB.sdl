    Name  = "SMM_COMMUNICATION_BUFFER_SIZE"
    Value  = "0x20000"
    Help  = "Size of the EfiRuntimeServicesData buffer allocated by firmware and used by OS applications for SMM communication"
    TokenType = Integer
    TargetH = Yes
    Range  = "0 - 0FFFFFFFFh"
End

TOKEN
    Name  = "SPI_IMAGE_SIZE"
    Value  = "0x4000000"
    Help  = "This size should be flash size add ME size"
    TokenType = Integer
    TargetH = Yes
End
#Fix added for SW SMI failure END

TOKEN
	Name  = "PEI_USB_KEYBOARD_CONNECT_DELAY_MS"
	Value  = "120"
	Help  = "Some devices may connect lately, add delay time to wait usb keyboard are connected. The unit of the token is MS."
	TokenType = Integer
	TargetH = Yes
	Token = "USB_PEI_KEYBOARD_SUPPORT" "=" "1"
End

#To Remove Duplicated NVME information displayed in Setup
ELINK
    Name  = "NvmeContollerNamelist"
    InvokeOrder = ReplaceParent
    Help  = "NvmeControllerNamelist should be in the format of{ {{P2P DevNum, P2P FunNum},...,{NvmeCtrl DevNum, NvmeCtrl FunNum},{0xFF, 0xFF}}, {OEM Name(With-in double quotes)} },P2P means PCI to PCI Bridge(no need to port for OnBoard NvmeControllers). List of all Dev and Fun number of PCI Hardware device nodes till NvmeController should be ported.Example: '{ {{0x1d, 0x00},{0xc0,0x00},{0xFF, 0xFF}}, {Nvme1(With-in Double Quotes)} },' "
End

OUTPUTREGISTER
    Name = "NvmeControllerNameElinkFile"
    Path = "Build"
    File = "NvmeControllerNameElink.h"
    Template = "..\..\AmiModulePkg\Nvme\NvmeDynamicSetup\NvmeDevInfoElink.txt"
End
#To Remove Duplicated NVME information displayed in Setup
