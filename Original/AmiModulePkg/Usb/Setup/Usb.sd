            flags  = 0,
            key    = 0;

        SEPARATOR
                                        //<(EIP102493+)

        text
            help   = STRING_TOKEN(STR_EMPTY),
            text   = STRING_TOKEN(STR_USB_CONTROLLERS_ENABLED),
            flags  = 0,
            key    = 0;
