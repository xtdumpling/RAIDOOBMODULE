  AmiCompatibilityPkg/AmiCompatibilityPkg.dec
  AmiCryptoPkg/AmiCryptoPkg.dec
  
[LibraryClasses]
  PeimEntryPoint
  PeiServicesLib
  AmiPeiLib
  PeiCryptLib
  DebugLib

[Ppis]
