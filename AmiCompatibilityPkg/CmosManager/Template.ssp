//---------------------------------------------------------------------------
// Internal CMOS Manager non-volatile status
//
//---------------------------------------------------------------------------

NvramField ( CMOS_MGR_BATTERY_BAD )
	OptionBits  = 1
	Managed     = NO
EndNvramField

NvramField ( CMOS_MGR_CHECKSUM_BAD )
	OptionBits  = 1
	Managed     = NO
EndNvramField

NvramField ( CMOS_MGR_DEFAULTS_LOADED )
	OptionBits  = 1
	Managed     = NO
EndNvramField

NvramField ( CMOS_MGR_FIRST_BOOT_DETECTED )
	OptionBits  = 1
	Managed     = NO
EndNvramField



